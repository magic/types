{
  "name": "@magic/types",
  "version": "0.0.1",
  "description": "typechecking library",
  "author": "Jascha Ehrenreich <jascha@jaeh.at>",
  "license": "AGPL-3.0",
  "main": "src/index.js",
  "scripts": {
    "start": "nyc -a node test/",
    "test": "VERBOSE=true nyc -a node test/",
    "format": "prettier --write '{src,test}/**/*.js'",
    "format:check": "prettier --list-different '{src,test}/**/*.js'"
  },
  "prettier": {
    "semi": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/magic/types"
  },
  "bugs": {
    "url": "https://github.com/magic/types/issues"
  },
  "homepage": "https://github.com/magic/types",
  "devDependencies": {
    "@magic/test": "github:magic/test",
    "nyc": "11.6.0",
    "prettier": "1.11.1"
  }
}
