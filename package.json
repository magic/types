  "license": "AGPL-3.0",
  "description": "typechecking library",
  "main": "src/index.js",
  "types": "./types/index.d.ts",
  "type": "module",
  "scripts": {
    "start": "t",
    "build": "tsc && node scripts/strip-js-extension.js && npm run format && echo 'Types generated in dist/*'",
    "prepublishOnly": "npm run build",
    "build:docs": "NODE_ENV=production magic build",
    "prod": "NODE_ENV=production magic build serve",
    "clean": "magic clean",
    "serve": "magic serve",
    "dev": "magic dev",
    "test": "t",
    "format": "f -w --exclude docs",
    "format:check": "f --exclude docs",
    "calls": "calls"
  },
  "engines": {
    "node": ">=18"
  },
  "engineStrict": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/magic/types"
  },
  "bugs": {
    "url": "https://github.com/magic/types/issues"
  },
  "files": [
    "src",
    "types"
  ],
  "devDependencies": {
    "@magic-modules/git-badges": "0.0.12",
    "@magic-modules/light-switch": "0.0.12",
    "@magic-modules/no-spy": "0.0.9",
    "@magic-modules/pre": "0.0.12",
    "@magic-themes/docs": "0.0.15",
    "@magic/core": "0.0.156",
    "@magic/format": "0.0.68",
    "@magic/fs": "0.0.31",
    "@magic/test": "0.2.20",
    "@types/node": "24.5.2",
    "typescript": "5.9.2"
  },
  "keywords": [
    "magic",
