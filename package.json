{
  "name": "types",
  "version": "0.0.1",
  "description": "typechecking library",
  "author": "Jascha Ehrenreich <jascha@jaeh.at>",
  "license": "AGPL-3.0",
  "main": "src/index.js",
  "scripts": {
    "start": "nyc -a node test/",
    "test": "VERBOSE=true nyc -a node test/",
    "lint": "esw -w ./src ./test",
    "lint:fix": "eslint --fix ./src ./test"
  },
  "eslintConfig": {
    "extends": "standard",
    "rules": {
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "brace-style": [
        "error",
        "stroustrup"
      ],
      "semi": [
        "error",
        "never"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/magic/types"
  },
  "bugs": {
    "url": "https://github.com/magic/types/issues"
  },
  "homepage": "https://github.com/magic/types",
  "devDependencies": {
    "test": "git+http://git@localhost:7005/test"
  }
}
